    "compilerOptions": {
        "module": "commonjs",
        "target": "es5",
        "outDir": "lib",
        "rootDir": "src",
        "noImplicitAny": true,
        "sourceMap": false,
        "noUnusedLocals": true,
        "noUnusedParameters": false,
        "strictNullChecks": true,
        "pretty": true,
        "noImplicitReturns": true
    }
